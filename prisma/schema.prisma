generator client {
  provider = "prisma-client-js"
}



datasource db {
  provider = "postgresql"
  url      = "postgresql://app_user:app_password@localhost:5432/app_database?schema=public"
}
model Product {
  id         Int      @id @default(autoincrement())
  brand      String
  productName String
  upc12      BigInt   @unique
  createdAt DateTime @default(now()) @db.Timestamp
}